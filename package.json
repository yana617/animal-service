{
   "name": "animal-service",
   "version": "1.0.0",
   "description": "Animal CRUD",
   "main": "index.js",
   "scripts": {
      "test": "jest --runInBand",
      "test:docker": "chmod +x ./bin/test.sh && NODE_ENV=test ./bin/test.sh",
      "dev": "NODE_ENV=development nodemon index.ts",
      "dev:docker": "docker compose up",
      "start": "NODE_ENV=production ts-node index.ts",
      "typeorm": "typeorm-ts-node-commonjs -d ./src/database/index.ts",
      "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/update-animals-table -d ./src/database/index.ts",
      "lint": "eslint . --ext .ts",
      "build:swagger": "swagger-cli bundle documentation/swagger.yaml --outfile swagger.yaml --type yaml"
   },
   "keywords": [
      "animal",
      "hos"
   ],
   "author": "Yana Sidarava",
   "license": "ISC",
   "dependencies": {
      "@aws-sdk/client-s3": "3.637.0",
      "axios": "1.7.5",
      "body-parser": "1.20.2",
      "class-validator": "0.14.0",
      "cors": "2.8.5",
      "dotenv": "16.3.1",
      "express": "4.19.2",
      "express-validator": "7.1.0",
      "js-yaml": "4.1.0",
      "morgan": "1.10.0",
      "multer": "1.4.5-lts.1",
      "multer-s3": "3.0.1",
      "pg": "8.5.1",
      "reflect-metadata": "0.1.13",
      "swagger-ui-express": "5.0.1",
      "typeorm": "0.3.20"
   },
   "devDependencies": {
      "@types/cors": "2.8.13",
      "@types/express": "4.17.17",
      "@types/jest": "29.5.12",
      "@types/morgan": "1.9.5",
      "@types/multer": "^1.4.12",
      "@types/multer-s3": "3.0.3",
      "@types/node": "16.11.10",
      "@typescript-eslint/eslint-plugin": "6.5.0",
      "aws-sdk-client-mock": "^4.1.0",
      "eslint": "8.48.0",
      "eslint-config-standard-with-typescript": "39.0.0",
      "eslint-plugin-import": "2.28.1",
      "eslint-plugin-n": "16.0.2",
      "eslint-plugin-promise": "6.1.1",
      "faker": "5.5.3",
      "jest": "29.7.0",
      "nock": "13.5.4",
      "nodemon": "3.1.2",
      "supertest": "7.0.0",
      "ts-jest": "29.1.4",
      "ts-node": "10.7.0",
      "typescript": "4.9.5"
   }
}
